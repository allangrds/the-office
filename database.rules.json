{
  "rules": {
    "rooms": {
      ".read": "auth.uid != null",
      "$roomId": {
        ".write": "!data.exists()",
        "users": {
          "$uid": {
            ".write": "auth.uid == $uid"
          }
        }
      }
    },
    "users": {
      "$uid": {
        ".read": "auth.uid != null",
        ".write": "auth.uid == $uid"
      }
    }
  }
}